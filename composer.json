{
  "name": "adminaut/adminaut",
  "description": "Adminaut is the open-source platform for rapid development of PHP applications with the automatic construction of administration backend. It's built on the top of PHP, Zend Framework, Doctrine ORM and other frameworks.",
  "license": "MIT",
  "authors": [
    {
      "name": "Petr Malecha",
      "email": "petr.malecha@mfcc.cz",
      "homepage": "http://www.bedi.cz",
      "role": "Developer"
    },
    {
      "name": "Josef Soušek",
      "email": "josef.sousek@mfcc.cz",
      "role": "Developer"
    },
    {
      "name": "Matúš Petrofčík",
      "email": "matus@petrofcik.eu",
      "homepage": "https://www.petrofcik.eu",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/adminaut/adminaut",
  "keywords": [
    "adminaut",
    "admin",
    "zf3-module"
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "composer/installers": "~1.0",
    "zendframework/zend-mvc": "^3.0.1",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.0.0",
    "zendframework/zend-form": "^2.10",
    "zendframework/zend-authentication": "^2.5",
    "zendframework/zend-filter": "^2.7",
    "zendframework/zend-navigation": "^2.8",
    "zendframework/zend-crypt": "^3.2",
    "zendframework/zend-i18n": "^2.7",
    "zendframework/zend-json": "^3.0",
    "zendframework/zend-file": "^2.7",
    "zendframework/zend-mail": "^2.8",
    "zendframework/zend-paginator": "^2.7",
    "zendframework/zend-permissions-acl": "^2.6",
    "zendframework/zend-session": "^2.8",
    "zendframework/zend-uri": "^2.5",
    "doctrine/doctrine-orm-module": "~1.1",
    "neilime/zf2-twb-bundle": "^3.1",
    "league/flysystem": "~1.0",
    "bushbaby/flysystem": "~3.0",
    "smottt/wideimage": "~1.1",
    "league/iso3166": "^2.0",
    "almasaeed2010/adminlte": "^2.3"
  },
  "autoload": {
    "psr-0": {
      "Adminaut\\": "src/"
    }
  },
  "extra": {
    "installer-paths": {
      "www_root/adminaut/themes/{$name}": [
        "almasaeed2010/adminlte"
      ]
    }
  }
}
